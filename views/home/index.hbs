
        {{# each times}}

          <div class="row">
            {{# each this}}

              <div class="col-sm-6 col-md-4">
                  <div class="thumbnail">
                      <img src= "data:image/jpeg;base64," alt="..." class="img-responsive" alt="..." class="img-responsive">
                      <div class="caption">
                          <h3>Open Door Detected at: </h3>

                          <div class="clearfix">
                              <div class="Time pull-left">Time: {{this.CurrentDateAndTime}}</div>

                          </div>
                      </div>
                  </div>
              </div>
              {{/each}}
          </div>
        {{/each}}

        <p>
        Time: <span id="time"></span>
      </p>

  <script>

  const time_url = 'http://worldclockapi.com/api/json/gmt/now';

  async function getTime(){
    const response = await fetch(time_url);
    const data = await response.json();
    document.getElementById('time').textContent = data.currentDateTime;
    console.log(data.currentDateTime);
  }


  getTime();

  </script>
